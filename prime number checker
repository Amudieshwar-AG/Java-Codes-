package AG;

class L {
    int i, n;

    void performOperations() {
        if (n <= 1) {
            System.out.println(n + " is not a prime number");
            return;
        }
w
        for (i = 2; i < n; i++) {
            if (n % i == 0) {
                System.out.println(n + " is not a prime number");
                return; // exit method earlyw
            }
        }

        // If loop completes without finding a factor
        System.out.println(n + " is a prime number");
    }
}

public class AG2 {
    public static void main(String[] args) {
        L obj = new L();
        obj.n = 2;// Change the number to test
        obj.performOperations();
    }
}
