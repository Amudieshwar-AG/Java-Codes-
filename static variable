class usestatic
{
    static int a=3, b;
    static void meth(int x){
        System.out.println("x="+x);
        System.out.println("a="+a);
        System.out.println("b="+b);
    }
    static
    {
        System.out.println("static block initialized");
        b=a*4;
    }
}
class Main {
    public static void main(String[] args) {
        usestatic.meth(42);
    }
}
